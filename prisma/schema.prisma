generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int    @id @default(autoincrement())
  name        String
  numberPhone String
  times Time[]
}

model Barber {
  id          Int    @id @default(autoincrement())
  name        String
  email       String @unique
  password    String
  numberPhone String
}

model Time {
  id Int @id @default(autoincrement())
  available Boolean  @default(false)
  date DateTime
  user User @relation(fields: [userId], references: [id])
  userId Int
}
